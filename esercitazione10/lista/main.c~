#pragma once
#include "lista.h"
#include "lista.c"
#include <stdlib.h>
#include <stdio.h>
int main(){
	TipoLista l=listaVuota();
	TipoLista ll=cons(9 , l );
	TipoLista l1=cons(8,ll);
	TipoLista l2=cons(7,l1);
	TipoLista l3=cons(6,l2);
	printf("l3\n");
	printa(l3);
	TipoLista s=listaVuota();
	TipoLista s1=cons(1 , s );
	TipoLista s2=cons(2,s1);
	TipoLista s3=cons(3,s2);
	TipoLista s4=cons(4,s3);
	printf("\ns4\n");
	printa(s4);
	
	TipoLista ris=appendNtimes(s4 , 21, 4);
	printf("\nris\n");
	printa(ris);
}
